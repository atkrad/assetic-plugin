        "source": "https://github.com/hurad/assetic-plugin"
    },
    "require": {
        "php": ">=5.4.19",
        "cakephp/cakephp": "3.0.*@dev",
        "composer/installers": "1.0.*",
        "kriswallsmith/assetic": "1.1.*",
        "fxp/composer-asset-plugin": "1.0.*@dev"
