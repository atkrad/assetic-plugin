{
    "name": "hurad/assetic-plugin",
    "description": "Asset Management for CakePHP",
    "homepage": "https://github.com/hurad/assetic-plugin",
    "type": "hurad-plugin",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Hurad Community",
            "homepage": "https://github.com/hurad/assetic-plugin/graphs/contributors"
        }
    ],
    "support": {
